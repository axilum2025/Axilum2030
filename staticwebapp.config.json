{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "/fonctions.html", "/outils.html"]
  },
  "platform": {
    "apiRuntime": "node:20"
  },
  "mimeTypes": {
    ".json": "application/json"
  },
  "globalHeaders": {
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "DENY",
    "X-XSS-Protection": "1; mode=block"
  }
}
